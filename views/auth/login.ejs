<head>
	<link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css" integrity="sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u" crossorigin="anonymous">
	<link rel='stylesheet' href='/stylesheets/auth.css'>
	<link href='//fonts.googleapis.com/css?family=Open+Sans:400,300,600,700,800' rel='stylesheet' type='text/css'>
</head>

<div class="pageView loginSignup">
	<div class="main-agileits">
		<div class="mainw3-agileinfo">
			<!-- login form -->
			<div class="login-form">
				<div class="login-agileits-top">
					<form action="/login/" method="post">
						<p>Username </p>
						<input id="input1" type="text" class="email" name="username" placeholder="Email address" required=""/>
						<p>Password</p>
						<input id="input2" type="password" class="password" name="password" placeholder=".........." required=""/>
						<% if (typeof(message) != "undefined") {%>
							<div class="error-message"><%= message %></div>
						<% } %>
						<input type="submit" value="Login">
					</form>
					<p class="account-message">
				    Not have an account? <a href="/signup">Sign up</a>
				  </p>
				</div>
				<div class="login-agileits-bottom">
					<h6><a href="#">Forgot password?</a></h6>
				</div>

				<a href="/auth/pinterest">Login With Pinterest</a>

			</div>
		</div>
	</div>


	<!-- //main -->
	<!-- copyright -->
	<div class="w3copyright-agile hide">
