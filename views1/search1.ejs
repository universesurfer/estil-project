<head>
	<link rel="stylesheet" href="stylesheets/stylesearch.css">
</head>

<div class="pageView">
	<h2>Find a stylist</h2>

	<div id="container">
		<div class="search">
		  <h4>Filter by:</h4>
		  <div class="dropdown">
			  <label for="price">Price</label>
			  <button name="price" class="btn btn-default dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
			  	Price range <span class="caret"></span>
			  </button>
			  <ul class="dropdown-menu dropdown-menu-right" aria-labelledby="dropdownMenu1">
					<li><a data-target="#">€</a></li>
					<li><a data-target="#">€€</a></li>
					<li><a data-target="#">€€€</a></li>
		  	</ul>
		  </div>


	  <div class="dropdown">
		  <label for="day">Day</label>
		  <button name="day" class="btn btn-default dropdown-toggle" type="button" id="dropdownMenu2" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
		  	Pick a day <span class="caret"></span>
		  </button>
		  <ul class="dropdown-menu dropdown-menu-right" id="availability" aria-labelledby="dropdownMenu1">
				<li><a data-target="#">Monday</a></li>
				<li><a data-target="#">Tuesday</a></li>
				<li><a data-target="#">Wednesday</a></li>
				<li><a data-target="#">Thursday</a></li>
				<li><a data-target="#">Friday</a></li>
				<li><a data-target="#">Saturday</a></li>
				<li><a data-target="#">Sunday</a></li>
				<li><a data-target="#">Every Day</a></li>
		  </ul>
	 </div>

	  <div class="dropdown">
	    <label for="spot">My home or salon</label>
	    <button name="spot" class="btn btn-default dropdown-toggle" type="button" id="dropdownMenu3" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
	    	Pick a place <span class="caret"></span>
	    </button>
	    <ul class="dropdown-menu dropdown-menu-right" aria-labelledby="dropdownMenu1">
				<li><a data-target="#">Home</a></li>
				<li><a data-target="#">Salon</a></li>
	    </ul>
		</div>

	  <div class="dropdown">
	    <label for="service">Services</label>
	    <button name="spot" class="btn btn-default dropdown-toggle" type="button" id="dropdownMenu4" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
	    Pick a service <span class="caret"></span>
	    </button>
	    <ul class="dropdown-menu dropdown-menu-right" aria-labelledby="dropdownMenu1">
				<li><a data-target="#">Cut</a></li>
				<li><a data-target="#">Blow Dry</a></li>
				<li><a data-target="#">Color</a></li>
	    </ul>
	  </div>


		<div class="dropdown">
			<label for="service">Expertise</label>
			<button name="spot" class="btn btn-default dropdown-toggle" type="button" id="dropdownMenu5" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
				Hair style for <span class="caret"></span>
			</button>
			<ul class="dropdown-menu dropdown-menu-right" id="expertise" aria-labelledby="dropdownMenu1">
				<li><a data-target="#">Women</a></li>
				<li><a data-target="#">Men</a></li>
				<li><a data-target="#">Any</a></li>
			</ul>
		</div>
	</div>



	 <div id="map"></div>
	 <input type="text" id="pac-input" name="search" placeholder="Choose your location">

	<script async defer src='<%=apiKey%>'></script>
	<script src="javascripts/search.js"></script>

</div>
